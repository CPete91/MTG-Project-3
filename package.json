  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "auth0-js": "^9.10.2",
    "aws-sdk": "^2.450.0",
    "axios": "^0.21.1",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "passport": "^0.4.0",
    "react-router-dom": "^5.0.0"
  }
}
